#include <stdio.h>

/**
*main - entry point for main function
*
*Return: 0
*/
int main(void)
{
	int pass, x = 10;

	while(x != 0)
	{
	_putchar("iput the password: ");
	scanf("%d", &pass);
	if (pass == 1234)
	{
	_putchar("correct password");
	x = 0;
	else
	{
	_putchar("wrong password, try another");
	}
	return (0);

}
